{
	"Name" : "One In The Chamber",
	"Description": "Allows One In The Chamber in servers gamemodes",
	"Version": "1.0.0",
	"LoadPriority": 1,
	"RequiredOnClient": true,

	"Scripts": [
		// one in the chamber
		{
			"Path": "gamemodes/sh_gamemode_chamber.nut",
			"RunOn": "( CLIENT || SERVER ) && MP",
			"ClientCallback": {
				"Before": "Sh_GamemodeChamber_Init"
			},

			"ServerCallback": {
				"Before": "Sh_GamemodeChamber_Init"
			}
		},
		{
			"Path": "gamemodes/_gamemode_chamber.nut",
			"RunOn": "SERVER && MP"
		},
        {
			"Path": "gamemodes/cl_gamemode_chamber.nut",
			"RunOn": "CLIENT && MP"
		},
	],

	"Localisation": [
		"resource/chamber_custom_%language%.txt"
	]
}